case $(hostname -s) in
  *)   export GOPATH="${BOXEN_SRC_DIR}golang" ;;
esac

# PATH includes the go binaries.
export PATH=$GOPATH/bin:$PATH

# Load up autojump.
[[ -s $(brew --prefix)/etc/profile.d/autojump.sh ]] && . $(brew --prefix)/etc/profile.d/autojump.sh

eval "$(rbenv init --no-rehash - zsh)"
